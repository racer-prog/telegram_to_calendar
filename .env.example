
# Токен бота, полученный от @BotFather
BOT_TOKEN=ваш_токен_бота

# Настройки CalDAV для Google Calendar
# URL должен быть в формате: https://apidata.googleusercontent.com/caldav/v2/ваш_email/events
CALDAV_URL=https://apidata.googleusercontent.com/caldav/v2/ваш_email/events
CALDAV_USERNAME=ваш_email@gmail.com
# Для Google Calendar используйте пароль приложения, а не обычный пароль аккаунта
# Создайте его в настройках безопасности: https://myaccount.google.com/security -> Пароли приложений
CALDAV_PASSWORD=ваш_пароль_приложения

# Уровень логирования (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
